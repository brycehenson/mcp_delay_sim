Cell[TextData[{
 "Define the surface interesctions\ny direction along the cyl\nx axis of tilt\
\nsurface of the mcp\nz=y ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"cos", "[", "\[Alpha]", "]"}], 
    RowBox[{"Sin", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "9cc89eb4-5c3f-4c6e-ad9e-35f8a31b6a0a"],
 "\nparametric cyl (u,v)\ny=v\nx=Cos[u]\nz=Sin[u]\nthe intersection is thus\n\
y=z ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"Sin", "[", "\[Alpha]", "]"}], 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "8c41b79a-df79-40a2-8b0c-e162d1a8617d"],
 "=Sin[u] ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"Sin", "[", "\[Alpha]", "]"}], 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "1cda00cd-4178-4da0-92c0-2c9a79c655b3"],
 "\nx=Cos[u]\nz=Sin[u]\nRotate into the plane of the mcp \
x\[CloseCurlyQuote],y\[CloseCurlyQuote],z\[CloseCurlyQuote]\nx\
\[CloseCurlyQuote]=Cos[u]\ny\[CloseCurlyQuote]=(Sin[u] ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"Sin", "[", "\[Alpha]", "]"}], 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "d7b62ae9-d92b-4676-8f7c-af19277c6b56"],
 ")Cos[\[Alpha]]-(Sin[u])Sin[\[Alpha]]\nz\[CloseCurlyQuote]=(Sin[u] ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"Sin", "[", "\[Alpha]", "]"}], 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "8d752d52-65b8-4f38-84f1-430189710d1e"],
 ")Sin[\[Alpha]]+(Sin[u])Cos[\[Alpha]]\nsimplify\nx\[CloseCurlyQuote]=Cos[u]\n\
y\[CloseCurlyQuote]=0\nz\[CloseCurlyQuote]= ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"Sin", "[", "u", "]"}], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"Sin", "[", "\[Alpha]", "]"}], ")"}], "2"]}], 
    RowBox[{"Cos", "[", "\[Alpha]", "]"}]], TraditionalForm]],ExpressionUUID->
  "5930b2cb-cc56-4e2e-9708-40976d3fa15f"],
